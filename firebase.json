
{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1",
      "runtime": "nodejs20",
      "memory": "1GiB",
      "minInstances": 0,
      "maxInstances": 100,
      "concurrency": 80,
      "cpu": 1,
      "name": "siren-h2y45",
      "environment": {
        "STRIPE_SECRET_KEY": "REPLACE_WITH_YOUR_STRIPE_SECRET_KEY",
        "STRIPE_WEBHOOK_SECRET": "REPLACE_WITH_YOUR_STRIPE_WEBHOOK_SECRET",
        "NEXT_PUBLIC_GOOGLE_CLIENT_ID": "REPLACE_WITH_A_CORRECTLY_CONFIGURED_GOOGLE_CLIENT_ID"
      }
    }
  }
}
