
{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1",
      "runtime": "nodejs20",
      "memory": "1GiB",
      "minInstances": 0,
      "maxInstances": 100,
      "concurrency": 80,
      "cpu": 1,
      "name": "siren-h2y45"
    },
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin-allow-popups"
          }
        ]
      }
    ]
  }
}
